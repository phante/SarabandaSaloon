                                  <contextMenu>
                                      <ContextMenu>
                                          <items>
                                              <MenuItem mnemonicParsing="false" onAction="#handleMoveToSongList" text="Sposta sulle manche normali" />
                                          </items>
                                      </ContextMenu>
                                  </contextMenu>
                              </TableView>
                     </items>
